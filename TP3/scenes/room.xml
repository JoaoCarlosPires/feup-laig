<lsf>
    <initials>
        <reference length="5" />    <!-- axis length -->
        <root id="room" />      <!-- identifier of root node of the scene graph -->
    </initials>

    <views default="Ambient">
        <perspective id="Ambient" near="0.1" far="500" angle="45">
                <from x="35" y="30" z="0" />
                <to x="2" y="5" z="0" />
        </perspective>
        <perspective id="Front Camera" near="0.1" far="300" angle="80">
            <from x="30" y="5" z="0" />
            <to x="0" y="5" z="0" />
        </perspective>
        <perspective id="Board" near="0.1" far="50" angle="30">
            <from x="30" y="25" z="0" />
            <to x="0" y="0" z="0" />
        </perspective>
        <perspective id="Player 1" near="0.1" far="50" angle="15">
            <from x="30" y="25" z="0.7" />
            <to x="0" y="0" z="0.7" />
        </perspective>
        <perspective id="Player 2" near="0.1" far="50" angle="15">
            <from x="-20" y="25" z="0.7" />
            <to x="10" y="0" z="0.7" />
        </perspective>
        <perspective id="Scorer" near="0.1" far="50" angle="30">
            <from x="15" y="10" z="0.0" />
            <to x="0" y="10" z="0" />
        </perspective>
    </views>

    <illumination>
        <ambient r="1" g="1" b="1" a="1.0" />                       <!-- global ambient: black -->
        <background r="0.4" g="0.4" b="0.4" a="1.0" />              <!-- background color: gray -->
    </illumination>

    <lights>
        <light id="Back Light">                                      <!-- light identifier -->
            <enable value="1" />                                    <!-- enable/disable -->
            <position x="-12" y="10" z="1" w="0.0" />                <!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0" g="0" b="0" a="1.0" />                   <!-- ambient component -->
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />             <!-- diffuse component -->
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />            <!-- specular component -->
        </light>
        <light id="Left Light">                                      <!-- light identifier -->
            <enable value="1" />                                    <!-- enable/disable -->
            <position x="0" y="10" z="15" w="1.0" />                <!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0" g="0" b="0" a="1.0" />                   <!-- ambient component -->
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />             <!-- diffuse component -->
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />            <!-- specular component -->
        </light>
        <light id="Right Light">                                     <!-- light identifier -->
            <enable value="1" />                                    <!-- enable/disable -->
            <position x="0" y="10" z="-11" w="1.0" />               <!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0" g="0" b="0" a="1.0" />                   <!-- ambient component -->
